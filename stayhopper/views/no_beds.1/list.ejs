<%-include('../shared/header.ejs',{active:'properties',parent:'hotels-management'})%>
    <div class="content-inner">
        <!-- Page Header-->
        <header class="page-header">
            <div class="container-fluid d-flex justify-content-between align-items-center">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item">
                            <a href="#">Hotel Management</a>
                        </li>
                        <li class="breadcrumb-item" aria-current="page">
                            <a href="/generalsettings">General Settings</a>
                        </li>
                        <li class="breadcrumb-item active fw-700" aria-current="page">No of Beds</li>
                    </ol>
                </nav>
            </div>
        </header>
         <div class="container-fluid">
            <div class="row wrapper mt-0 mb-0 pt-4 bg-white pb-2 px-4">
                    <div class="form-header mt-2">
                        No of Beds
                    </div>
                </div>
                <div class="col-sm-12 mt-2 mt-md-3 mb-2 mb-md-2">
                    <div class="header-buttons d-flex justify-content-end">
                        <button type="button" class="btn btn-primary mr-md-4" data-toggle="modal" data-target=".bd-example-modal-lg" data-whatever="@mdo">
                            <i class="icon-plus-circle"></i> Add New</button>
                        <div class="modal bd-example-modal-lg" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
                            aria-hidden="true">
                            <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
                                <div class="modal-content">
                                    <div class="modal-header">
                                        <h5 class="modal-title" id="exampleModalLabel">New message</h5>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>
                                    <div class="modal-body">
                                        <div class="row wrapper mt-4 mb-5 bg-white">
                                            <div class="col-sm-12 mb-5 mb-md-5 clearfix">
                                                <div class="form-header-block fs-26">
                                                    Item Name
                                                </div>
                                            </div>

                                            <div class="col-sm-6 mt-2 mt-md-3 mb-2 mb-md-2">
                                                <div class="form-inline row">
                                                    <label for="inputPassword" class="col-sm-3 col-form-label">Item Name</label>
                                                    <div class="col-sm-9">
                                                        <input type="text" class="form-control form-control-lg border-secondary" id="inputPassword" placeholder="Please Enter Item">
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="col-sm-6 mt-2 mt-md-3 mb-2 mb-md-2">
                                                <div class="form-inline row">
                                                    <label for="inputPassword" class="col-sm-3 col-form-label">Select Icon</label>
                                                    <div class="col-sm-9">

                                                        <select class="border-secondary cs-select cs-skin-elastic">
                                                            <option value="" disabled selected>Select Icon</option>
                                                            <option value="france" data-class="flag-france">House</option>
                                                            <option value="brazil" data-class="flag-brazil">Villa</option>
                                                            <option value="argentina" data-class="flag-argentina">Apartment</option>
                                                            <option value="south-africa" data-class="flag-safrica">Flat</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="col-sm-12 mt-3 mt-md-5 mb-2 mb-md-2">
                                                <div class="header-buttons d-flex justify-content-center">
                                                    <button type="button" class="btn btn-primary btn-lg mr-md-4 pl-5 pr-5">SAVE</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-lg-12 col-md-12 col-sm-12 col-12 text-left" style="overflow-x:auto">
                    <table class="table table-hover" style="overflow-x:scroll;">
                        <thead class="mt-5">
                            <tr>
                                <th>Item Name</th>
                                <th></th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            <% no_beds.forEach(guest=>{ %>
                                <tr>
                                    <td style="width: 75%" class="d-flex align-items-center col-10"><%=guest.name%>
                                        <img class="ml-4" width="30px" src="data:image/png;base64,"
                                            alt="" />
                                    </td>
                                    <td style="width: 6.33%">
                                        <a id="show-panel" href="#">
                                            <i class="icon-edit font-lg"></i>
                                        </a>
                                    </td>
                                    <td style="width: 8.33%" class="col-2">
                                        <a id="show-panel" href="#" data-toggle="modal" data-target=".bd-example-modal" data-whatever="@mdo">
                                            <i class="icon-trash-2 font-lg"></i>
                                        </a>
                                    </td>
                                </tr>
                            <% }); %>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div class="modal bd-example-modal" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <div class="row wrapper mt-4 mb-5 bg-white justify-content-center">
                            <div class="col-sm-12 mb-5 mb-md-5 clearfix">
                                <div class="form-header-block fs-26 text-center text-secondary">
                                    Are you sure you want to remove this property
                                </div>
                            </div>

                            <div class="col-sm-4 mt-2 mt-md-3 mb-2 mb-md-2 d-flex justify-content-center align-items-center">
                                <button type="button" class="btn btn-secondary btn-block btn-lg mr-md-4 pl-5 pr-5">Delete</button>
                            </div>

                            <div class="col-sm-4 mt-2 mt-md-3 mb-2 mb-md-2 d-flex justify-content-center align-items-center close opacity-1" data-dismiss="modal"
                                aria-label="Close">
                                <button type="button" class="btn btn-secondary btn-block btn-lg mr-md-4 pl-5 pr-5">Cancel</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <%-include('../shared/footer.ejs')%>